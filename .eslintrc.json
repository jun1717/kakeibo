{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "no-undef": 0,
    "arrow-parens": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-param-reassign": 0
  },
  "parser": "babel-eslint"
}